{
  "build": {
    "content": [
      {
        "files": ["smpp/**/*.md", "smpp/**/*.yml"]
      },
      {
        "files": ["mm7/**/*.md", "mm7/**/*.yml"]
      },
      {
        "files": ["toc.yml","*.md"]
      }
    ],
    "resource": [
      {
        "files": ["smpp/images/**"]
      }
    ],
    "dest": "_site",
    "globalMetadata": 
     {
      "_appTitle": "Documentaion, API and code examples",
      "_enableSearch": true,
      "_appFooter": "<span>Copyright Â© 2010-2017 InetLab e.U. <br>Generated by <strong>DocFX</strong></span>"
     },
    "template": [
      "default","template"
    ],
    "sitemap":{
        "baseUrl": "https://docs.inetlab.com",
        "priority": 0.1,
        "changefreq": "weekly",
        "fileOptions":{
            "smpp/**/*.yml": {
                "priority": 0.3
            },
            "smpp/**/*.md": {
                "priority": 0.3
            },
            "mm7/**/*.yml": {
                "priority": 0.3
            },
            "mm7/**/*.md": {
                "priority": 0.3
            },
            "*.md": {
                "priority": 0.3
            }

        }
    }
  }
}