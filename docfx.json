{
  "build": {
    "content": [
      {
        "files": ["smpp/**/*.md", "smpp/**/*.yml"]
      },
      {
        "files": ["mm7/**/*.md", "mm7/**/*.yml"]
      },
      {
        "files": ["toc.yml","*.md"]
      }
    ],
    "resource": [
      {
        "files": ["smpp/images/**"]
      }
    ],
    "dest": "_site",
    "globalMetadata": 
     {
      "_appTitle": "Documentaion, API and code examples",
      "_enableSearch": "true",
      "_appFooter": "<span>Copyright Â© 2010-2017 InetLab e.U. <br>Generated by <strong>DocFX</strong></span>"
     }
   
    ,
    "fileMetadataFiles": [],
    "template": [
      "default","template"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false
  },
  "pdf": {
    "name": "documentation",
    "content": [
      {
        "files": ["smpp/**/*.md", "smpp/**/*.yml"]
      },
      {
        "files": ["mm7/**/*.md", "mm7/**/*.yml"]
      },
      {
        "files": [
         "smpp/toc.yml",
         "mm7/toc.yml",
        ]
      }
    ],
    "resource": [
      {
        "files": ["smpp/images/**"]
      }
    ],

    "dest": "_site_pdf"
  }
}