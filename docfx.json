{
  "build": {
    "content": [
      {
         "files": ["smpp/*.{md,yml}"]
      },
      {
        "files": ["*.{md,yml}","**/*.{md,yml}"],
        "src": "smpp/v1/", 
        "dest": "smpp/v1/",
        "version": "v1"
      },
      {
        "files": ["*.{md,yml}", "**/*.{md,yml}"],
        "src": "smpp/v2/",
        "dest": "smpp/v2/",
        "version": "v2"
      },
      {
        "files": ["mm7/**/*.md", "mm7/**/*.yml"]
      },
      {
        "files": ["toc.yml", "index.md"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"],
        "src": "smpp/v1",
        "dest": "smpp/v1",
        "version": "v1"
      }
    ],
    "dest": "_site",
    "globalMetadata": 
     {
      "_appTitle": "Documentaion, API and code examples",
      "_enableSearch": true,
      "_appFooter": "<span>Copyright Â© 2010-2018 InetLab e.U. <br>Generated by <strong>DocFX</strong></span>"
     },
    "template": [
      "default","templates/inetlab","templates/memberpage"
    ],
    "sitemap":{
        "baseUrl": "https://docs.inetlab.com",
        "priority": 0.1,
        "changefreq": "weekly",
        "fileOptions":{
            "smpp/**/*.yml": {
                "priority": 0.3
            },
            "smpp/**/*.md": {
                "priority": 0.3
            },
            "mm7/**/*.yml": {
                "priority": 0.3
            },
            "mm7/**/*.md": {
                "priority": 0.3
            },
            "*.md": {
                "priority": 0.3
            }

        }
    }
  }
}